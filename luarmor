local kf="LHub_Key.txt"
local lu="https://sdkapi-public.luarmor.net/library.lua"
local sid="42f92b639d0394ac5806de25b07e26d5"
local ll="https://api.luarmor.net/files/v4/loaders/42f92b639d0394ac5806de25b07e26d5.lua"
local gk="https://ads.luarmor.net/get_key?for=-LnDzsGFTqXOu"
local function t(s)return(s or ""):match("^%s*(.-)%s*$")end
local function r()return isfile(kf)and t(readfile(kf))end
local function w(k)writefile(kf,t(k))getgenv().script_key=t(k)end
local function d()if isfile(kf)then delfile(kf)end end
local function tl(k)
if not k then return false end
k=t(k)
if#k~=32 then warn("Key len invalid:",#k)return false end
getgenv().script_key=k
local ok,a=pcall(function()return loadstring(game:HttpGet(lu))()end)
if not ok or not a then warn("Luarmor load fail")return false end
a.script_id=sid
local ok2,s=pcall(function()return a.check_key(k)end)
print("API:",ok2,s and s.code,s and s.message)
if not ok2 or not s then warn("API fail:",s)return false end
if s.code=="KEY_VALID"then loadstring(game:HttpGet(ll))()return true
else warn("Key invalid:",s.code,s.message)return false end
end
local function Load()
local k=r()
if k and tl(k)then return end
local R=loadstring(game:HttpGet("https://raw.githubusercontent.com/LawlietSHub/Library/refs/heads/main/customFluent"))()
local W=R:CreateWindow({Name="LH Key",LoadingTitle="Enter Key",LoadingSubtitle="by L",ToggleUIKeybind="L"})
local T=W:CreateTab("Key")
local u=""
T:CreateButton({Name="Get Key",Callback=function()setclipboard(gk)end})
T:CreateInput({Name="Enter",PlaceholderText="Paste 32-char key",Callback=function(i)u=t(i)end})
T:CreateButton({Name="Verify",Callback=function()if u==""then return end w(u)Load()end})
end
Load()
